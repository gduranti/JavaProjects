<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.unisinos.pf2.nltest.ide.controller.ExecutionPanelController">
   <top>
      <ToolBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <items>
            <ProgressBar fx:id="progressBar" prefHeight="18.0" prefWidth="248.0" progress="0.0" />
            <Button fx:id="cancelButton" cancelButton="true" mnemonicParsing="false" text="Cancelar Execução" />
            <Button fx:id="printButton" alignment="CENTER" mnemonicParsing="false" text="Imprimir Resultado" />
        </items>
      </ToolBar>
   </top>
   <center>
      <TreeTableView fx:id="treeResult" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <columns>
          <TreeTableColumn fx:id="testUnityColumn" prefWidth="337.0" text="Unidade" />
          <TreeTableColumn fx:id="testResultColumn" minWidth="0.0" prefWidth="67.0" text="Resultado" />
            <TreeTableColumn fx:id="testMessageColumn" prefWidth="195.0" text="Mensagem" />
        </columns>
      </TreeTableView>
   </center>
</BorderPane>
